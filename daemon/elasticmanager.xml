<service>
	<id>elasticmanager.exe</id>
	<name>Elastic Manager</name>
	<description>Serviciu de management al misiunilor si sesiunilor din elasticsearch</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>--max_old_space_size=4096</argument>
	<argument>C:\KIT_THOR\Elastic-manager\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\KIT_THOR\Elastic-manager\index.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>Elastic Manager wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>60</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>DESKTOP-I3LNQ5J</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\KIT_THOR\Elastic-manager</workingdirectory>
</service>